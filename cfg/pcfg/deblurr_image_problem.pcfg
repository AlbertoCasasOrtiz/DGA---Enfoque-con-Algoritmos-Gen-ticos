# Número de decimales que tendrán las celdas del kernel [>0].
num_decimals: 3
# Dimensiones del kernel. [>1, número impar].
dim_x: 3
dim_y: 3
# Valores que pueden tomar las celdas del kernel [min_val < max_val].
min_val: -3
max_val: 3
# Path de la imagen que vamos a enfocar.
image_path: images/torre-blurr.jpg
# Función que medirá el desenfoque [Variance of Laplacian, Modified Laplacian, Tenengrad].
measure_function: Variance of Laplacian